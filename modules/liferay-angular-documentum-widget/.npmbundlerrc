{
	"config": {
		"imports": {
			"frontend-js-node-shims": {
				"url": ">=0.11.0"
			},
			"client-js-node-shims": {
				"fs": "1.0.0",
				"http": "1.7.0",
				"https": "1.7.0",
				"stream": "2.0.2",
				"readable-stream": "3.1.1",
				"zlib": "0.0.3"
			}
		}
	},
	"exclude": {
		"pdfjs-dist": [
			"cmaps/**/*",
			"image_decoders/**/*",
			"lib/**/*",
			"web/**/*"
		]
	},
	"create-jar": {
		"auto-deploy-portlet": true,
		"web-context": "/liferay-angular-documentum-widget",
		"output-dir": "dist"
	},
	"dump-report": true,
	"process-serially": true
}